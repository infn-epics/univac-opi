<display version="2.0.0">
  <name>VacuumChannel</name>
  <width>410</width>
  <height>30</height>
  <widget type="rectangle" version="2.0.0">
    <name>Rectangle_5</name>
    <x>129</x>
    <width>221</width>
    <height>30</height>
    <line_width>1</line_width>
    <line_color>
      <color name="Grid" red="128" green="128" blue="128">
      </color>
    </line_color>
    <background_color>
      <color name="Background" red="255" green="255" blue="255">
      </color>
    </background_color>
  </widget>
  <widget type="textupdate" version="2.0.0">
    <name>PressureReadout</name>
    <pv_name>$(P):$(R):$(PVNAME)</pv_name>
    <x>134</x>
    <y>6</y>
    <width>95</width>
    <format>3</format>
    <precision>3</precision>
    <show_units>false</show_units>
    <horizontal_alignment>1</horizontal_alignment>
  </widget>
  <widget type="rectangle" version="2.0.0">
    <name>Rectangle_4</name>
    <width>129</width>
    <height>30</height>
    <line_width>1</line_width>
    <line_color>
      <color name="Grid" red="128" green="128" blue="128">
      </color>
    </line_color>
    <background_color>
      <color name="Background" red="255" green="255" blue="255">
      </color>
    </background_color>
  </widget>
  <widget type="label" version="2.0.0">
    <name>SensorName</name>
    <text>$(NAME)</text>
    <y>6</y>
    <width>125</width>
    <font>
      <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
      </font>
    </font>
  </widget>
  <widget type="symbol" version="2.0.0">
    <name>Mode_1</name>
    <pv_name>loc://$(P):$(R)_TYPE&lt;VLong&gt;($(TYPE))</pv_name>
    <symbols>
      <symbol>img/ionpump.svg</symbol>
      <symbol>img/ccg.svg</symbol>
      <symbol>img/pig.svg</symbol>
    </symbols>
    <x>238</x>
    <y>3</y>
    <width>40</width>
    <height>23</height>
    <actions>
      <action type="open_display">
        <description>Open Histo</description>
        <file>histoval.bob</file>
        <target>window</target>
      </action>
    </actions>
    <run_actions_on_mouse_click>true</run_actions_on_mouse_click>
    <tooltip>Open Graph</tooltip>
    <fallback_symbol>img/gas.svg</fallback_symbol>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_12</name>
    <text>$(ZONE)</text>
    <x>288</x>
    <y>5</y>
    <width>90</width>
    <font>
      <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
      </font>
    </font>
  </widget>
  <widget type="action_button" version="3.0.0">
    <name>CustomControl</name>
    <actions>
      <action type="open_display">
        <description>Open Display</description>
        <file>$(OPI)</file>
        <target>window</target>
      </action>
    </actions>
    <pv_name>loc://control_$(LCID)&lt;VString&gt;("$(OPI)")</pv_name>
    <text>Ctrl</text>
    <x>350</x>
    <y>5</y>
    <width>60</width>
    <height>20</height>
    <rules>
      <rule name="Enable" prop_id="visible" out_exp="false">
        <exp bool_exp="pvStr0!=&quot;&quot;">
          <value>true</value>
        </exp>
        <exp bool_exp="pvStr0==&quot;&quot;">
          <value>false</value>
        </exp>
        <pv_name>$(pv_name)</pv_name>
      </rule>
    </rules>
    <tooltip>$(actions)</tooltip>
  </widget>
</display>
